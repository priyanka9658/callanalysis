<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>com.callanalysis.apr</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
   
  </welcome-file-list>
  
  <servlet>
      <servlet-name>dialpad</servlet-name>
      <servlet-class>com.callanalysis.dial.DialServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>dialpad</servlet-name>
  <url-pattern>/Dial</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
      <servlet-name>daywl</servlet-name>
      <servlet-class>com.callanalysis.dwcl.DayWeeklServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>daywl</servlet-name>
  <url-pattern>/DWL</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
      <servlet-name>daywh</servlet-name>
      <servlet-class>com.callanalysis.dwch.DayweekServlet </servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>daywh</servlet-name>
  <url-pattern>/DW</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>dayhl</servlet-name>
      <servlet-class>com.callanalysis.hdcl.DayhourlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>dayhl</servlet-name>
  <url-pattern>/DhL</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>dayhh</servlet-name>
      <servlet-class>com.callanalysis.hdch.DayhourServlet </servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>dayhh</servlet-name>
  <url-pattern>/Dh</url-pattern>
  </servlet-mapping>
  
</web-app>